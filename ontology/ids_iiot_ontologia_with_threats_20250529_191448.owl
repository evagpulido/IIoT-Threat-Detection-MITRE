    <rdf:type rdf:resource="http://universidad.es/tfm/ids-iiot/ontologia#Mitigacion"/>
    <ids:tieneID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M0951</ids:tieneID>
    <ids:tieneNombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Update Software</ids:tieneNombre>
    <ids:tieneDescripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Analice regularmente los sistemas expuestos a Internet en busca de vulnerabilidades y aplique parches rápidamente cuando se descubran vulnerabilidades críticas.</ids:tieneDescripcion>
    <rdfs:label>M0951</rdfs:label>
    <rdfs:comment>Mitigación MITRE ATT&amp;CK for ICS</rdfs:comment>
  </rdf:Description>
