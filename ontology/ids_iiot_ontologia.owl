    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#utilizaTecnica">
        <rdfs:label>utilizaTecnica</rdfs:label>
        <rdfs:domain rdf:resource="#AmenazaDetectada"/>
        <rdfs:range rdf:resource="#Tecnica"/>
